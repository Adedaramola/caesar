{
    "name": "adedaramola/ceasar",
    "description": "",
    "keywords": [
        "laravel",
        "paystack",
        "billing"
    ],
    "homepage": "https://github.com/adedaramola/ceasar",
    "license": "Apache-2.0",
    "type": "library",
    "authors": [
        {
            "name": "Adedaramola Adetimehin",
            "email": "mailstokindle@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.3",
        "illuminate/database": "^10.3",
        "illuminate/http": "^10.3",
        "illuminate/routing": "^10.3",
        "illuminate/support": "^10.3",
        "illuminate/view": "^10.3"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Adedaramola\\Caesar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adedaramola\\Caesar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adedaramola\\Caesar\\CaesarServiceProvider"
            ],
            "aliases": {
                "Payspark": "Adedaramola\\Caesar\\Caesar"
            }
        }
    }
}
